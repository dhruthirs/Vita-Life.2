{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "LOCAL_MONGO_URI": "@local_mongo_uri",
    "USE_LOCAL_DB": "@use_local_db"
  }
}
